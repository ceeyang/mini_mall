{
  "source": {
    "include": ["./src"],
    "includePattern": "\\.js$",
    "exclude": ["node_modules", "src/**/*.test.js"]
  },
  "opts": {
    "destination": "./docs/api",
    "recurse": true,
    "template": "node_modules/docdash"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true
    }
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  }
}
